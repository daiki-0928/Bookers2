<h2>Users</h2>

 <table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   <%@users.each do |user|%>
    <tr>
     <td><%= image_tag 'sample-author1.jpg' %></td>
     <td><%= user.name %></td>
    </tr>

   <%end%>
  </tbody>
 </table>