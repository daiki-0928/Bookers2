<%= render 'books/newbook' ,book: @newbook %>

<h2>Users</h2>

 <table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   <%@users.each do |user|%>
    <tr>
     <td><%= image_tag user.get_profile_image(40,40) %></td>
     <td><%= user.name %></td>
     <td><%= link_to user_path(user.id) do %>show
         <% end %>
     </td>
    </tr>

   <%end%>
  </tbody>
 </table>